{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "yiisoft/yii2-queue": "^2.1",
        "yiisoft/yii2-redis": "~2.0.0",
        "yiisoft/yii2-elasticsearch": "~2.0.0",
        "yiisoft/yii2-faker": "^2.0",
        "kartik-v/yii2-widgets": "dev-master",
        "kartik-v/yii2-dynagrid": "dev-master",
        "kartik-v/yii2-datecontrol": "dev-master",
        "kartik-v/yii2-tree-manager": "dev-master",
        "kartik-v/yii2-ipinfo": "dev-master",
        "kartik-v/yii2-export": "dev-master",
        "kartik-v/yii2-builder": "dev-master",
        "kartik-v/yii2-password": "dev-master",
        "kartik-v/yii2-context-menu": "dev-master",
        "kartik-v/yii2-label-inplace": "dev-master",
        "kartik-v/yii2-detail-view": "dev-master",
        "kartik-v/yii2-editable": "dev-master",
        "wbraganca/yii2-dynamicform": "*",
        "pheme/yii2-settings": "*",
        "codeception/codeception": "^2.5",
        "mult1mate/cron-manager": "^1.1",
        "symfony/process": "^3.4",
        "zxbodya/yii2-gallery-manager": "*@dev",
        "symfony/finder": "^3.4",
        "lajax/yii2-translate-manager": "^1.7",
        "alexandernst/yii2-device-detect": "0.0.12"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
